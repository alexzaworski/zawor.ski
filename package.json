{
  "scripts": {
    "scss": "node-sass --output-style compressed -o docs/css src/scss",
    "autoprefix": "postcss --use autoprefixer docs/css/style.css --replace",
    "inline": "inliner --skip-absolute-urls src/index.html > docs/index.html",
    "build": "webpack -p && npm run scss && npm run autoprefix && npm run inline"
  },
  "dependencies": {
    "animejs": "^1.1.1",
    "webfontloader": "^1.6.26"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "inliner": "^1.9.5",
    "node-sass": "^3.10.1",
    "postcss-cli": "^2.6.0",
    "webpack": "^1.13.2"
  }
}
